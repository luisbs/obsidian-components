{
  "private": true,
  "name": "obsidian-components",
  "version": "0.5.0",
  "license": "MIT",
  "description": "Plugin to use template components on render time",
  "keywords": [
    "obsidian",
    "codeblock",
    "component",
    "template"
  ],
  "author": {
    "name": "Luis Brayan",
    "url": "https://github.com/luisbs"
  },
  "type": "module",
  "module": "./dist/main.js",
  "scripts": {
    "watch": "vite build --watch --mode development",
    "build": "vite build",
    "build:version": "node ./scripts/version-bump.mjs && git add manifest.json versions.json",
    "lint": "run-s lint:eslint lint:prettier",
    "lint:fix": "run-s lint:eslint:fix lint:prettier:fix lint:ts",
    "lint:eslint": "eslint src",
    "lint:eslint:fix": "eslint --fix src",
    "lint:prettier": "prettier --check src",
    "lint:prettier:fix": "prettier --write src",
    "lint:ts": "tsc -noEmit -skipLibCheck"
  },
  "dependencies": {
    "obsidian-fnc": "github:luisbs/obsidian-fnc#v1.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "builtin-modules": "^3.3.0",
    "eslint": "^8.57.0",
    "npm-run-all": "^4.1.5",
    "obsidian": "^1.5.7",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3",
    "vite": "^5.3.3",
    "vite-plugin-static-copy": "^1.0.6"
  }
}
